        <Protocol Name="NETFLOW_V5" Icon=":/npg/resource/udp.png" Dependence="UDP">
            <Category Name="Header">
                <Field Name="version" Type="int" InputMethod="none" Length="1" Text="version" DefaultValue="5">
                <Field Name="count" Type="int" InputMethod="edit" Length="2">
                <Field Name="sys_uptime" Type="int" InputMethod="edit" Length="4">
                <Field Name="unix_secs" Type="int" InputMethod="edit" Length="4">
                <Field Name="unix_nsecs" Type="int" InputMethod="edit" Length="4">
                <Field Name="flow_sequence" Type="int" InputMethod="edit" Length="4">
                <Field Name="engine_type" Type="int" InputMethod="edit" Length="1">
                <Field Name="engine_id" Type="int" InputMethod="edit" Length="1">
                <Field Name="sampling_interval" Type="int" InputMethod="edit" Length="2">
            </Category>
            <Category Name="Record" Many="true">
                <Field Name="srcaddr" Type="ip" InputMethod="edit">
                <Field Name="dstaddr" Type="ip" InputMethod="edit">
                <Field Name="nexthop" Type="ip" InputMethod="edit">
                <Field Name="input" Type="int" InputMethod="edit" Length="2">
                <Field Name="output" Type="int" InputMethod="edit" Length="2">
                <Field Name="dPkts" Type="int" InputMethod="edit" Length="4">
                <Field Name="dOctets" Type="int" InputMethod="edit" Length="4">
                <Field Name="first" Type="int" InputMethod="edit" Length="4">
                <Field Name="last" Type="int" InputMethod="edit" Length="4">
                <Field Name="srcport" Type="int" InputMethod="edit" Length="2">
                <Field Name="dstport" Type="int" InputMethod="edit" Length="2">
                <Field Name="pad1" Type="int" InputMethod="none" Length="1" DefaultValue="0">
                <Field Name="tcp_flags" Type="int" InputMethod="edit" Length="1">
                <Field Name="prot" Type="int" InputMethod="edit" Length="1">
                <Field Name="tos" Type="int" InputMethod="edit" Length="1">
                <Field Name="src_as" Type="int" InputMethod="edit" Length="2">
                <Field Name="dst_as" Type="int" InputMethod="edit" Length="2">
                <Field Name="src_mask" Type="int" InputMethod="edit" Length="1">
                <Field Name="dst_mask" Type="int" InputMethod="edit" Length="1">
                <Field Name="pad1" Type="int" InputMethod="none" Length="2" DefaultValue="0">                
            </Category>
        </Protocol>
    </Protocols>
</Npg>
