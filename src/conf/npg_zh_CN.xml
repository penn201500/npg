                <Field Name="zone_name" Type="string" InputMethod="lineedit" ShowOnStart="false" Tip="策略域名称(字符串)"/>
                <Field Name="tail" Type="int" InputMethod="lineedit" Length="1" Editable="false" DefaultValue="0" ShowOnStart="true" Tip="字符串结束标志"/>
            </Category>
        </Protocol>
                <Protocol Name="YAMU_business" Dependence="TCP">
            <Category Name="报文头">
                <Field Name="version" Type="int" InputMethod="lineedit" DefaultValue="1" Length="1" Tip="版本，1个字节整型，该字节为0x01"/>
                <Field Name="cid" Type="int" InputMethod="lineedit" Length="2" DefaultValue="0"   Tip="操作号，2个字节整型"/>
                <Field Name="dt" Type="int" InputMethod="select" Length="1"  Tip="设备类型">
                    <Item Value="2" Text="FlowExport"/>
                    <Item Value="1" Text="DNsys"/>
                    <Item Value="3" Text="AppGateway"/>
                    <Item Value="4" Text="Recursive BIND"/>
                    <Item Value="5" Text="Recursive CNS"/>
                    <Item Value="6" Text="Authority DNsys"/>
                    <Item Value="7" Text="Authority BIND"/>
                    <Item Value="8" Text="DMS"/>
                </Field>
