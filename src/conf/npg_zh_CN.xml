                </Field>
                <Field Name="bl" Type="int" InputMethod="lineedit" Length="4" Tip="报文体长度，4个字节整型，有效值0~232，表示该报文的报文体的长度，单位byte"/>
                <Field Name="options" Type="string" InputMethod="lineedit" Length="7" DefaultValue="0000000" Editable="false" Tip="选项，7个字节，用于扩展。"/>
            </Category>
            <Category Name="报文体">
                <Field Name="length" Type="int" InputMethod="lineedit" Length="2" ShowOnStart="true" Tip="单元长度"/>
