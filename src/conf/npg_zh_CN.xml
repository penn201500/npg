<?xml version="1.0" encoding="utf-8"?>
<Npg>
    <Protocols>
        <Protocol Name="HTTP" Dependence="TCP">
            <Category Name="Request line" Tail="&#13;&#10;">
                <Field Name="type" Type="string" InputMethod="select" Tail="&#32;">
                    <Item Value="GET" Text="GET"/> 
                    <Item Value="POST" Text="POST"/> 
                    <Item Value="DELETE" Text="DELETE"/> 
                    <Item Value="UPDATE" Text="UPDATE"/> 
                </Field>
                <Field Name="uri" Type="string" InputMethod="lineedit" DefaultValue="/" Tail="&#32;"/>
                <Field Name="version" Type="string" InputMethod="lineedit" DefaultValue="HTTP/1.1"/>
            </Category>
            <Category Name="Header" Tail="&#13;&#10;">
                <Field Name="host" Type="string" InputMethod="lineedit" Prefix="HOST:" Tail="&#13;&#10;"/>
            </Category>
            <Category Name="Data">
                <Field Name="Content-Type" Type="string" InputMethod="lineedit" Prefix="Content-Type:" Optional="true" DefaultValue="application/x-www-form-urlencoded" Tail="&#13;&#10;"/>
                <Field Name="Content-Length" Type="string" InputMethod="lineedit" Prefix="Content-Length:"   Optional="true" Tail="&#13;&#10;"/>
                <Field Name="Connection" Type="string" InputMethod="lineedit" Prefix="Connection:" Optional="true" DefaultValue="close" Tail="&#13;&#10;"/>
                <Field Name="Data" Type="string" InputMethod="textedit" Optional="true" Tail="&#13;&#10;"/>
            </Category>
        </Protocol>
        <Protocol Name="Ether_arp" Dependence="ETHERNET" DependenceParam="ARP">
            <Category Name="Header">
                <Field Name="ar_hrd" Type="int" InputMethod="lineedit" Length="2" Editable = "false" DefaultValue="1" Tip="硬件地址格式"/>
                <Field Name="ar_pro" Type="int" InputMethod="lineedit" Length="2"  Editable = "false" DefaultValue="2048" Tip="协议地址格式"/>
                <Field Name="ar_hln" Type="int" InputMethod="lineedit" Length="1"  Editable = "false" DefaultValue="6" Tip="硬件地址长度"/>
                <Field Name="ar_pln" Type="int" InputMethod="lineedit" Length="1"  Editable = "false" DefaultValue="4" Tip="协议地址长度"/>
                <Field Name="ar_op" Type="int" InputMethod="select" Length="2" Editable = "false" Tip="ARP操作码">
                    <Item Value="1" Text="arp请求"/>
                    <Item Value="2" Text="arp响应"/>
                </Field>
            </Category>
            <Category Name="Data">
                <Field Name="arp_sha" Type="mac" InputMethod="lineedit" Tip="发送端硬件地址(MAC)"/>
                <Field Name="arp_spa" Type="ip" InputMethod="lineedit" Tip="发送端协议地址(IP)"/>
                <Field Name="arp_tha" Type="mac" InputMethod="lineedit" Tip="目的端硬件地址(MAC)"/>
                <Field Name="arp_tpa" Type="ip" InputMethod="lineedit" Tip="目的端协议地址(IP)"/>
            </Category>
        </Protocol>
        <Protocol Name="Ether_rarp" Dependence="ETHERNET" DependenceParam="RARP">
            <Category Name="Header">
                <Field Name="ar_hrd" Type="int" InputMethod="lineedit" Length="2" Editable = "false" DefaultValue="1" Tip="硬件地址格式"/>
                <Field Name="ar_pro" Type="int" InputMethod="lineedit" Length="2"  Editable = "false" DefaultValue="2048" Tip="协议地址格式"/>
                <Field Name="ar_hln" Type="int" InputMethod="lineedit" Length="1"  Editable = "false" DefaultValue="6" Tip="硬件地址长度"/>
                <Field Name="ar_pln" Type="int" InputMethod="lineedit" Length="1"  Editable = "false" DefaultValue="4" Tip="协议地址长度"/>
                <Field Name="ar_op" Type="int" InputMethod="select" Length="2" Editable = "false" Tip="ARP opcode (command)">
                    <Item Value="3" Text="rarp请求"/>
                    <Item Value="4" Text="rarp响应"/>
                </Field>
            </Category>
            <Category Name="Data">
                <Field Name="arp_sha" Type="mac" InputMethod="lineedit" Tip="发送端硬件地址(MAC)"/>
                <Field Name="arp_spa" Type="ip" InputMethod="lineedit" Tip="发送端协议地址(IP)"/>
                <Field Name="arp_tha" Type="mac" InputMethod="lineedit" Tip="目的端硬件地址(MAC)"/>
                <Field Name="arp_tpa" Type="ip" InputMethod="lineedit" Tip="目的端协议地址(IP)"/>
            </Category>
        </Protocol>
        <Protocol Name="ICMP" Dependence="IP" DependenceParam="ICMP">
            <Category Name="Header">
                <Field Name="type" Type="int" InputMethod="lineedit" Length="1" Tip="type"/>
                <Field Name="code" Type="int" InputMethod="lineedit" Length="1" Tip="code"/>
                <Field Name="checknum" Type="int" InputMethod="lineedit" Length="2" DefaultValue="checknum" Tip="The sum of a pseudo header of information from the icmp header and the data,\n padded with zero octets at the end(If zero, the program will automatically generate)"/>
            </Category>
            <Category Name="Data">
                <Field Name="id" Type="int" InputMethod="lineedit" Length="2" Optional="true" ShowOnStart="true" DefaultValue="pid" Tip="Unique id for this process"/>
                <Field Name="seq" Type="int" InputMethod="lineedit" Length="2" Optional="true" ShowOnStart="true" DefaultValue="seq" Tip="Sequence"/>
                <Field Name="otime" Type="int" InputMethod="lineedit" Length="4" Optional="true" ShowOnStart="true" DefaultValue="second" Tip="seconds from 1970-1-1"/>
                <Field Name="rtime" Type="int" InputMethod="lineedit" Length="4" Optional="true" ShowOnStart="true" DefaultValue="millisecond" Tip="millisecond from the start of this second"/>
                <Field Name="other" Type="int" InputMethod="lineedit" Length="2" Optional="true" Tip="User defined field"/>
            </Category>
        </Protocol>
        <Protocol Name="ICMP_PING" Dependence="IP" DependenceParam="ICMP">
            <Category Name="Header">
                <Field Name="type" Type="int" InputMethod="select" Editable = "false" Length="1" Tip="type">
                    <Item Value="0" Text="ping response"/>
                    <Item Value="8" Text="ping request"/>
                </Field>
                <Field Name="code" Type="int" InputMethod="lineedit" Editable = "false" Length="1" DefaultValue="0" Tip="code"/>
                <Field Name="checknum" Type="int" InputMethod="lineedit" Length="2" DefaultValue="checknum" Tip="The sum of a pseudo header of information from the icmp header and the data, padded with zero octets at the end(If zero, the program will automatically generate)"/>
            </Category>
            <Category Name="Data">
                <Field Name="id" Type="int" InputMethod="lineedit" Length="2" DefaultValue="pid" Tip="Unique id for this process"/>
                <Field Name="seq" Type="int" InputMethod="lineedit" Length="2" DefaultValue="seq" Tip="Sequence"/>
                <Field Name="otime" Type="int" InputMethod="lineedit" Length="4" DefaultValue="second" Tip="seconds from 1970-1-1"/>
                <Field Name="rtime" Type="int" InputMethod="lineedit" Length="4" DefaultValue="millisecond" Tip="millisecond from the start of this second"/>
            </Category>
            
        </Protocol>
        <Protocol Name="RAW_UDP" Dependence="IP" DependenceParam="UDP">
            <Category Name="Header">
                <Field Name="src_port" Type="int" InputMethod="lineedit" Length="2" Tip="源端口"/>
                <Field Name="dst_port" Type="int" InputMethod="lineedit" Length="2" Tip="目的端口"/>
                <Field Name="length" Type="int" InputMethod="lineedit" Length="2"  Tip="数据包长度，包含8字节的包头，最小长度为8"/>
                <Field Name="checknum" Type="int" InputMethod="lineedit" Length="2" DefaultValue="0" Tip="校验和（包含首部和数据）（可选的）"/>
            </Category>
            <Category Name="Data">
                <Field Name="data" Type="string" InputMethod="textedit" Tip="数据"/>
            </Category>
        </Protocol>
        <Protocol Name="NETFLOW_V5" Dependence="UDP">
            <Category Name="Header">
                <Field Name="version" Optional="false" Type="int" InputMethod="lineedit" Editable = "false" Length="2" Text="version" DefaultValue="5" Tip="NetFlow export format version number"/>
                <Field Name="count" Type="int" InputMethod="lineedit" Length="2" Tip="Number of flows exported in this packet (1-30)"/>
                <Field Name="sys_uptime" Type="int" InputMethod="lineedit" Length="4" DefaultValue="0" Tip="Current time in milliseconds since the export device booted"/>
                <Field Name="unix_secs" Type="int" InputMethod="lineedit" Length="4" DefaultValue="second" Tip="Current count of seconds since 0000 UTC 1970"/>
                <Field Name="unix_nsecs" Type="int" InputMethod="lineedit" Length="4" DefaultValue="0" Tip="Residual nanoseconds since 0000 UTC 1970"/>
                <Field Name="flow_sequence" Type="int" InputMethod="lineedit" Length="4" Tip="Sequence counter of total flows seen"/>
                <Field Name="engine_type" Type="int" InputMethod="lineedit" Length="1" Tip="Type of flow-switching engine"/>
                <Field Name="engine_id" Type="int" InputMethod="lineedit" Length="1" Tip="Slot number of the flow-switching engine"/>
                <Field Name="sampling_interval" Type="int" InputMethod="lineedit" Length="2" Tip="First two bits hold the sampling mode; remaining 14 bits hold value of sampling interval"/>
            </Category>
            <Category Name="Record" Many="true">
                <Field Name="srcaddr" Type="ip" InputMethod="lineedit" Tip="Source IP address"/>
                <Field Name="dstaddr" Type="ip" InputMethod="lineedit" Tip="Destination IP address"/>
                <Field Name="nexthop" Type="ip" InputMethod="lineedit" Tip="IP address of next hop router"/>
                <Field Name="input" Type="int" InputMethod="lineedit" Length="2" Tip="SNMP index of input interface"/>
                <Field Name="output" Type="int" InputMethod="lineedit" Length="2" Tip="SNMP index of output interface"/>
                <Field Name="dPkts" Type="int" InputMethod="lineedit" Length="4" Tip="Packets in the flow"/>
                <Field Name="dOctets" Type="int" InputMethod="lineedit" Length="4" Tip="Total number of Layer 3 bytes in the packets of the flow"/>
                <Field Name="first" Type="int" InputMethod="lineedit" Length="4" Tip="SysUptime at start of flow"/>
                <Field Name="last" Type="int" InputMethod="lineedit" Length="4" Tip="SysUptime at the time the last packet of the flow was received"/>
                <Field Name="srcport" Type="int" InputMethod="lineedit" Length="2" Tip="TCP/UDP source port number or equivalent"/>
                <Field Name="dstport" Type="int" InputMethod="lineedit" Length="2" Tip="TCP/UDP destination port number or equivalent"/>
                <Field Name="pad1" Type="int" InputMethod="lineedit" Length="1" DefaultValue="0" Tip="Unused (zero) bytes"/>
                <Field Name="tcp_flags" Type="int" InputMethod="lineedit" Length="1" Tip="Cumulative OR of TCP flags"/>
                <Field Name="prot" Type="int" InputMethod="lineedit" Length="1" Tip="IP protocol type (for example, TCP = 6; UDP = 17)"/>
                <Field Name="tos" Type="int" InputMethod="lineedit" Length="1" Tip="IP type of service (ToS)"/>
                <Field Name="src_as" Type="int" InputMethod="lineedit" Length="2" Tip="Autonomous system number of the source, either origin or peer"/>
                <Field Name="dst_as" Type="int" InputMethod="lineedit" Length="2" Tip="Autonomous system number of the destination, either origin or peer"/>
                <Field Name="src_mask" Type="int" InputMethod="lineedit" Length="1" Tip="Source address prefix mask bits"/>
                <Field Name="dst_mask" Type="int" InputMethod="lineedit" Length="1" Tip="Destination address prefix mask bits"/>
                <Field Name="pad2" Type="int" InputMethod="lineedit" Length="2" DefaultValue="0" Tip="Unused (zero) bytes"/>          
            </Category>
        </Protocol>
        <Protocol Name="DNS" Dependence="UDP" RFC="1035">
            <Category Name="Header">
                <Field Name="id" Type="int" InputMethod="lineedit" Length="2" DefaultValue="seq" Tip="16位的唯一ID，该ID在查询中设置，并在响应中返回，便于请求端判断该响应对应哪个请求."/>
                <Field Name="op_flag" Type="int" InputMethod="none" Length="2"  Tip="操作标记">
                    <SubField Name="qr" Type="bit" InputMethod="select" Length="1"  Tip="标识请求(0)、响应(1)">
                        <Item Value="0" Text="请求"/>
                        <Item Value="1" Text="响应"/>
                    </SubField>
                    <SubField Name="opcode" Type="bit" InputMethod="select" Length="4"  Tip="查询操作类型.  该值在查询中设置，并在响应中返回">
                        <Item Value="0" Text="标准查询"/>
                        <Item Value="1" Text="反向查询"/>
                        <Item Value="2" Text="服务器状态查询"/>
                    </SubField>
                    <SubField Name="aa" Type="bit" InputMethod="select" Length="1"  Tip="授权回答 - 该名字服务器是否授权于该服务器">
                        <Item Value="0" Text="否"/>
                        <Item Value="1" Text="是"/>
                    </SubField>
                    <SubField Name="tc" Type="bit" InputMethod="select" Length="1"  Tip="可截断 - 使用UDP时，它表示当应答的总长度超过512字节时，只返回前512字节">
                        <Item Value="0" Text="否"/>
                        <Item Value="1" Text="是"/>
                    </SubField>
                    <SubField Name="rd" Type="bit" InputMethod="select" Length="1"  Tip="期望递归 - 该值在查询中设置，并在响应中返回">
                        <Item Value="0" Text="否"/>
                        <Item Value="1" Text="是"/>
                    </SubField>
                    <SubField Name="ra" Type="bit" InputMethod="select" Length="1"  Tip="可用递归-如果DNS服务器支持递归查询，则在响应中将该比特设置为1">
                        <Item Value="0" Text="否"/>
                        <Item Value="1" Text="是"/>
                    </SubField>
                    <SubField Name="zero" Type="bit" InputMethod="lineedit" DefaultValue="0" Editable="false" Length="3"  Tip="保留字段，必须为0"/>
                    <SubField Name="rcode" Type="bit" InputMethod="select" Length="4"  Tip="返回码">
                        <Item Value="0" Text="没有差错"/>
                        <Item Value="1" Text="格式错误"/>
                        <Item Value="2" Text="服务器错误"/>
                        <Item Value="3" Text="名字错误"/>
                        <Item Value="4" Text="未实现"/>
                        <Item Value="5" Text="拒绝"/>
                    </SubField>
                </Field>
                <Field Name="QDCOUNT" Type="int" InputMethod="lineedit" Length="2" DefaultValue="1" Tip="问题数"/>
                <Field Name="ANCOUNT" Type="int" InputMethod="lineedit" Length="2"  DefaultValue="0" Tip="资源记录数"/>
                <Field Name="NSCOUNT" Type="int" InputMethod="lineedit" Length="2"  DefaultValue="0" Tip="授权资源记录数"/>
                <Field Name="ARCOUNT " Type="int" InputMethod="lineedit" Length="2"  DefaultValue="0" Tip="额外资源记录数"/>
            </Category>
            <Category Name="Request" Many="true">
                <Field Name="count" Type="int" InputMethod="lineedit" Length="1" Optional="true" ShowOnStart="true" Tip="下一个字段（名称）的长度"/>
                <Field Name="name" Type="string" InputMethod="lineedit" Optional="true" ShowOnStart="true" Tip="查询名称"/>
                <Field Name="count_end" Type="int" InputMethod="lineedit" Length="1" Editable="false"  DefaultValue="0" Tip="名称结束标识"/>
                <Field Name="QTYPE" Type="int" InputMethod="select" Length="2" Tip="查询类型">
                        <Item Value="1" Text="A(IP ADDR)"/>
                        <Item Value="12" Text="PTR"/>
                </Field>
                <Field Name="QCLASS" Type="int" Length="2" InputMethod="lineedit" DefaultValue="1" Tip="查询类，通常设置为1, 表示网络地址"/>
            </Category>
            <Category Name="Response" Tip="响应" Many="true">
            </Category>
        </Protocol>
    </Protocols>
</Npg>
